.sliderImages {
  img {
    width: 100%;
    margin: 0px auto !important;
    text-align:center;
  }
  .slick-list {
    z-index: 10;
  }
  .slick-dots {
    position: absolute;
    width: 100%;
    text-align: center;
    z-index: 15;
    li {
      display: inline-block;
      *display: inline;
      *zoom: 1;
      padding: 0 3px;
      font: 0/0 a;
      color: transparent;
      text-shadow: none;
      &.slick-active, &:hover, &:focus {
        button {
          background: #10385F;
        }
      }
      button {
        width: 12px;
        height: 12px;
        font: 0/0 a;
        color: transparent;
        text-shadow: none;
        background: rgba(219, 97, 116, 0.6);
        -webkit-border-radius: 50% 50%;
        -moz-border-radius: 50% 50%;
        border-radius: 50% 50%;
      }
    }
  }
}