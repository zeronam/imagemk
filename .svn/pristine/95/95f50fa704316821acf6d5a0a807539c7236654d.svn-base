input[type="checkbox"] {
    display: none;
}
input[type="radio"] {
    display: none;
}
input[type="checkbox"] + span {
	width: 17px;
    height: 17px;
    vertical-align: -3px;
    display: inline-block;
    cursor: pointer;
    background: url(../image/input_check.jpg) left top no-repeat;
    margin-right: 5px;
}
input[type="checkbox"]:checked + span{
    background: url(../image/input_checked.jpg) left top no-repeat;
}
input[type="radio"] + span {
	width: 17px;
    height: 17px;
    vertical-align: -4px;
    display: inline-block;
    cursor: pointer;
    background: url(../image/radio.png) left top no-repeat;
    margin-right: 5px;
}
input[type="radio"]:checked + span{
    background: url(../image/radio_checked.png) left top no-repeat;
}
button {
	cursor: pointer;
}

/* POPUP */
#dialogue-region {
	display: none;
	text-align: center;
	position: relative;
    z-index: 9999;
	.bg_screen {
		background: rgba(0, 0, 0, 0.8);
	    width: 100%;
	    height: 100%;
	    display: block;
	    position: fixed;
	    top: 0;
	    left: 0;
	    z-index: 24;
	    overflow: auto;  
	    transition: all 0.5s ease 0s;
	    &.show {
	    	display: block;
	    }

	}
	.dialogue_changeicn {
		margin: auto; 
		position: fixed;
		z-index: 99;
		transition: all 0.5s ease 0s;
		display: block;
		top: 50%;
		// padding-bottom: 10px;
		width: 900px;
		margin-left:-450px;
		left:50%;
		margin-top:-240px;
		/*height:480px;*/
		/*overflow-y:auto;*/
		.svgContainer {
			display: inline-block;
			margin: 10px 10px 0 10px;
			svg {
				width: 80px;
				height: 80px;
				fill: #9e9e9e;
				cursor: pointer;
			}
			&:hover svg , &.selected svg {
				fill: #673a3a
			}
		}
		.inner_dialogue {
			height: 480px;
			overflow-y: auto;
		    background: #fff;
		    margin: 0 auto;
		    text-align: center;
		}
	}
	.inner_dialogue::-webkit-scrollbar {
	    -webkit-appearance: none;
	    width: 5px;
	}
	.inner_dialogue::-webkit-scrollbar-thumb {
		border-radius: 5px;
		background-color: rgba(0,0,0,0.4);
		-webkit-box-shadow: 0 0 1px rgba(255,255,255,0.4);
	}
}
.dialogue_confirm {
		margin: auto; 
		position: fixed;
		z-index: 99;
		transition: all 0.5s ease 0s;
		display: none;
		top: 50%;
		padding-bottom: 10px;
		width: 500px;
		margin-left:-250px;
		left:50%;
		height:100px;
		margin-top:-50px;
		.inner_dialogue {
		    background: #fff;
		    margin: 0 auto;
		    text-align: center;
		    padding:15px;
		    p {
		    	margin: 0 0 15px 0;
		    }
		    button {
		    	width: 100px;
			    height: 30px;
			    display: inline-block;
			    border-radius: 5px;
			    margin-right: 10px;
			    line-height: 30px;
			    font-size:12px;
		    }
		    .agree {
		    	background: #30acc6;
		    	color: #fff;
		    }
		    .cancel {
		    	background: #aeaeae;
		    }
		    label {
		    	font-size:11px;
		    	color: #666;
		    }
		    span {
		    	display:inline-block;
		    	vertical-align: middle;
		    }
		    .actbtnconfirm {
		    	margin-top:15px;
		    }
		}
}
