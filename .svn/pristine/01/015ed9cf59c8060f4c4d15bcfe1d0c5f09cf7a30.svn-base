//////// FROM CSS ////////
#LOGIN-MAIN {
	padding: 20px;
	text-align: center;
}

#LOGIN-MAIN label {
	font-size: 13px;
	line-height: 17px;
	float: left;
}

#LOGIN-MAIN #user-id,
#LOGIN-MAIN #password {
	width: 100%;
	border-radius: 5px;
	height: 26px;
}

#LOGIN-MAIN #user-id {
	margin-bottom: 14px;
}

#LOGIN-MAIN #password {
	margin-bottom: 40px;
}

#LOGIN-MAIN .login-btn {
	width: 100%;
	border-radius: 5px;
	height: 44px;
	font-size: 16px;
	border: none;
	font-weight: bold;
	margin-bottom: 18px;
}

#LOGIN-MAIN .cancel-btn,
#LOGIN-MAIN .forget {
	display: inline-block;
	font-size: 13px;
	line-height: 16px;
	margin-bottom: 45px;
	border-bottom:1px solid;
	margin: 0 auto 45px auto;
}

#LOGIN-MAIN .singup,
#LOGIN-MAIN .singup:visited {
	display: block;
	width: 100%;
	height: 44px;
	font-size: 16px;
	line-height: 40px;
	border: 2px solid;
	border-radius: 5px;
	margin-bottom: 30px;
	box-sizing: border-box;
}

#LOGIN-MAIN .cancel-btn {
	margin-bottom: 10px;
}

#LOGIN-MAIN .instruction {
	font-size: 10px;
}
